{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|vue)$",
    "excludePattern": "(node_modules|dist)"
  },
  "opts": {
    "destination": "jsdoc-out",
    "recurse": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "tags": {
    "allowUnknownTags": true
  }
}

